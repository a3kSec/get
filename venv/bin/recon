#!/home/0verlord/recon/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ReconForever','console_scripts','recon'
__requires__ = 'ReconForever'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ReconForever', 'console_scripts', 'recon')()
    )
